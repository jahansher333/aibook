{
  "version": 2,
  "builds": [
    {
      "src": "app/main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "app/main.py"
    }
  ],
  "env": {
    "GROQ_API_KEY": "@groq_api_key",
    "COHERE_API_KEY": "@cohere_api_key",
    "QDRANT_URL": "@qdrant_url",
    "QDRANT_API_KEY": "@qdrant_api_key",
    "NEON_DATABASE_URL": "@neon_database_url",
    "LITELLM_MODEL": "groq/llama-3.1-70b-instant"
  }
}
